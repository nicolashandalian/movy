{
	"name": "QubikaApp",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"android": "react-native run-android --mode developmentDebug",
		"android:staging": "react-native run-android --mode stagingDebug",
		"android:prod": "react-native run-android --mode productionDebug",
		"ios": "react-native run-ios --scheme \"QubikaAppDevelopment\"",
		"ios:staging": "react-native run-ios --scheme \"QubikaAppStaging\"",
		"ios:prod": "react-native run-ios --scheme \"QubikaApp\"",
		"lint:js": "eslint .",
		"lint:ts": "tsc",
		"lint": "npm run lint:js && npm run lint:ts",
		"start": "react-native start",
		"test": "jest",
		"prepare": "husky install",
		"pods": "cd ios && bundle exec pod install"
	},
	"dependencies": {
		"@react-native-async-storage/async-storage": "^1.23.1",
		"@react-native-community/netinfo": "^9.4.1",
		"@react-native/babel-preset": "^0.73.21",
		"@react-native/typescript-config": "^0.73.1",
		"@react-navigation/native": "^6.1.7",
		"@react-navigation/native-stack": "^6.9.13",
		"@reduxjs/toolkit": "^1.9.5",
		"@shopify/flash-list": "^1.6.2",
		"@tanstack/react-query": "^5.8.1",
		"axios": "^1.6.0",
		"date-fns": "^2.30.0",
		"react": "18.2.0",
		"react-native": "^0.73.6",
		"react-native-bootsplash": "^5.0.3",
		"react-native-config": "^1.5.1",
		"react-native-encrypted-storage": "^4.0.3",
		"react-native-flipper": "^0.212.0",
		"react-native-localization": "^2.3.2",
		"react-native-reanimated": "^3.6.3",
		"react-native-safe-area-context": "^4.7.1",
		"react-native-screens": "^3.24.0",
		"react-native-svg": "^13.14.0",
		"react-redux": "^8.1.2",
		"redux-flipper": "^2.0.2",
		"redux-persist": "^6.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.20.0",
		"@babel/preset-env": "^7.20.0",
		"@babel/runtime": "^7.20.0",
		"@react-native/eslint-config": "^0.73.2",
		"@react-native/metro-config": "^0.73.5",
		"@tanstack/eslint-plugin-query": "^5.6.0",
		"@testing-library/react-native": "^12.3.2",
		"@tsconfig/react-native": "^3.0.0",
		"@types/jest": "^29.5.8",
		"@types/react": "^18.2.6",
		"@types/react-test-renderer": "^18.0.0",
		"@typescript-eslint/eslint-plugin": "^6.9.0",
		"@typescript-eslint/parser": "^6.9.0",
		"babel-jest": "^29.6.3",
		"babel-plugin-module-resolver": "^5.0.0",
		"eslint": "^8.52.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-jest": "^27.6.0",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "7.28.0",
		"eslint-plugin-react-native": "4.0.0",
		"husky": "^8.0.0",
		"jest": "^29.6.3",
		"jetifier": "^2.0.0",
		"lint-staged": "^15.0.2",
